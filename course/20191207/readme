b B KB MB GB TB PB
 /8
b
function FileSizeb(size) {
    if(size < 8) {
        return size.toFixed(2) + "b";
    }
    size /= 8;
    var index = 0;
    var units = ["B", "KB", "MB", "GB", "TB", "PB"];

    while(size >= 1024) {
        size /= 1024;
        index += 1;
    }
    return size.toFixed(2) + units[index];
}
